{
  "pages": [
    "pages/welcome/welcome",
  
    "pages/selectCommunity/selectCommunity",
    "pages/selectCommunityDetail/selectCommunityDetail",
    "pages/index/index",
    "pages/login/login",
    "pages/community/community",
    "pages/door/door",
    "pages/message/message",
    "pages/my/my",
    "pages/parkingQuery/parkingQuery",
    "pages/payment/payment",
    "pages/doorControl/doorControl",
    "pages/qrCode/qrCode",
    "pages/visitorPass/visitorPass",
    "pages/chessRoomBooking/chessRoomBooking",
    "pages/repair/prerepair",
    "pages/guestForm/guestForm",
    "pages/parking/parking",
    "pages/residentAuth/residentAuth"
  ],
  "window": {
    "navigationBarTextStyle": "black",
    "navigationStyle": "custom",
    "backgroundTextStyle": "light",
    "backgroundColor": "#F6F6F6"
  },
  "tabBar": {
    "custom": true,
    "color": "#999999",
    "selectedColor": "#1989fa",
    "backgroundColor": "#ffffff",
    "list": [
      {
        "pagePath": "pages/index/index",
        "text": "首页",
        "iconPath": "images/home.png",
        "selectedIconPath": "images/home_active.png"
      },
      {
        "pagePath": "pages/community/community",
        "text": "社区",
        "iconPath": "images/community.png",
        "selectedIconPath": "images/community_active.png"
      },
      {
        "pagePath": "pages/door/door",
        "text": "开门",
        "iconPath": "images/door.png",
        "selectedIconPath": "images/door_active.png"
      },
      {
        "pagePath": "pages/message/message",
        "text": "信息",
        "iconPath": "images/message.png",
        "selectedIconPath": "images/message_active.png"
      },
      {
        "pagePath": "pages/my/my",
        "text": "我的",
        "iconPath": "images/my.png",
        "selectedIconPath": "images/my_active.png"
      }
    ]
  },
  "style": "v2",
  "renderer": "skyline",
  "rendererOptions": {
    "skyline": {
      "defaultComponentDefinition": {}
    }
  },
  "componentFramework": "glass-easel",
  "sitemapLocation": "sitemap.json",
  "lazyCodeLoading": "requiredComponents",
  "subpackages": [
    {
      "root": "fonts-package",
      "name": "fontsPackage",
      "pages": ["index"]
    }
  ],
  "preloadRule": {
    "pages/index/index": {
      "network": "all",
      "packages": ["fontsPackage"]
    }
  },
  "permission": {
    "scope.userInfo": {
      "desc": "您的信息将用于小程序登录"
    }
  }
}